<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:id="@+id/mypage_nan_icon_iv"
        android:src="@drawable/splash_icon3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginHorizontal="40dp"
        android:layout_marginTop="40dp"/>
    <ImageView
        android:id="@+id/mypage_change_info_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginHorizontal="30dp"
        android:src="@drawable/setting_icon"/>
    
    <ImageView
        android:id="@+id/mypage_board_iv"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:src="@drawable/board_icon"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="30dp"
        app:layout_constraintTop_toBottomOf="@id/mypage_nan_icon_iv"
        android:layout_marginTop="30dp"/>

    <ImageView
        android:id="@+id/mypage_pet_fox"
        android:layout_width="0dp"
        android:layout_height="250dp"
        android:layout_marginTop="300dp"
        android:src="@drawable/fox"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="80dp"
        android:layout_marginStart="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:visibility="gone"/>
    <ImageView
        android:id="@+id/mypage_pet_bird"
        android:layout_width="0dp"
        android:layout_height="250dp"
        android:layout_marginTop="300dp"
        android:src="@drawable/bird"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="80dp"
        android:layout_marginStart="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:visibility="gone"/>
    <ImageView
        android:id="@+id/mypage_pet_bear"
        android:layout_width="0dp"
        android:layout_height="250dp"
        android:layout_marginTop="300dp"
        android:src="@drawable/bear"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="80dp"
        android:layout_marginStart="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:visibility="gone"/>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="160dp"
        android:id="@+id/mypage_post_iv"
        android:src="@drawable/post_icon"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/mypage_list_iv"
        android:src="@drawable/list_icon"
        app:layout_constraintEnd_toEndOf="@id/mypage_change_info_iv"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:src="@drawable/list_letter"
        app:layout_constraintEnd_toEndOf="@id/mypage_list_iv"
        app:layout_constraintStart_toStartOf="@id/mypage_list_iv"
        app:layout_constraintBottom_toBottomOf="@id/mypage_list_iv"
        app:layout_constraintTop_toTopOf="@id/mypage_list_iv"/>
    


</androidx.constraintlayout.widget.ConstraintLayout>