<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:id="@+id/mypage_nan_icon_iv"
        android:src="@drawable/splash_icon3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginHorizontal="40dp"
        android:layout_marginTop="40dp"/>
    <ImageView
        android:id="@+id/mypage_change_info_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginHorizontal="30dp"
        android:src="@drawable/setting_icon"/>
    
    <ImageView
        android:id="@+id/mypage_board_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/board_icon"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="30dp"
        app:layout_constraintTop_toBottomOf="@id/mypage_nan_icon_iv"
        android:layout_marginTop="110dp"/>
    <TextView
        android:background="@color/transparent"
        android:textColor="#411010"
        android:textStyle="bold"
        android:text="게시판"
        android:layout_width="50dp"
        android:layout_height="30dp"
        app:layout_constraintTop_toBottomOf="@id/mypage_nan_icon_iv"
        app:layout_constraintStart_toStartOf="@id/mypage_nan_icon_iv"
        android:layout_marginTop="140dp"
        android:layout_marginHorizontal="30dp"/>

    <ImageView
        android:id="@+id/mypage_pet_iv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="300dp"
        android:src="@drawable/pet_icon"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="50dp"
        android:layout_marginStart="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mypage_post_iv"
        android:src="@drawable/post_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintTop_toBottomOf="@id/mypage_board_iv"
        android:layout_marginTop="470dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/grass1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginHorizontal="35dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/grass2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginHorizontal="35dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/grass3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginHorizontal="35dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/grass4"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginHorizontal="35dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mypage_list_iv"
        android:src="@drawable/list_icon"
        app:layout_constraintEnd_toEndOf="@id/mypage_change_info_iv"
        android:layout_marginTop="600dp"
        android:layout_marginHorizontal="140dp"
        android:layout_marginStart="250dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_pet_iv"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/list_letter"
        app:layout_constraintEnd_toEndOf="@id/mypage_change_info_iv"
        app:layout_constraintBottom_toBottomOf="@id/mypage_pet_iv"
        android:layout_marginTop="590dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginHorizontal="10dp"/>
    


</androidx.constraintlayout.widget.ConstraintLayout>