<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:id="@+id/mypage_nan_icon_iv"
        android:src="@drawable/splash_icon3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"/>
    <ImageView
        android:id="@+id/mypage_change_info_iv"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="30dp"
        android:src="@drawable/setting_icon"/>
    
    <ImageView
        android:id="@+id/mypage_board_iv"
        android:layout_width="118dp"
        android:layout_height="105dp"
        android:src="@drawable/board_icon"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="30dp"
        app:layout_constraintTop_toBottomOf="@id/mypage_nan_icon_iv"
        android:layout_marginTop="80dp"/>
    <TextView
        android:background="@color/transparent"
        android:textColor="#411010"
        android:textStyle="bold"
        android:text="게시판"
        android:layout_width="50dp"
        android:layout_height="30dp"
        app:layout_constraintTop_toBottomOf="@id/mypage_nan_icon_iv"
        app:layout_constraintStart_toStartOf="@id/mypage_nan_icon_iv"
        android:layout_marginTop="85dp"
        android:layout_marginStart="40dp"/>

    <ImageView
        android:id="@+id/mypage_pet_iv"
        android:layout_width="270dp"
        android:layout_height="350dp"
        android:layout_marginTop="270dp"
        android:layout_marginStart="15dp"
        android:src="@drawable/pet_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:layout_width="120dp"
        android:layout_height="190dp"
        android:id="@+id/mypage_post_iv"
        android:src="@drawable/post_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintTop_toBottomOf="@id/mypage_board_iv"
        android:layout_marginStart="15dp"
        android:layout_marginTop="470dp"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:src="@drawable/grass1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="37dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:src="@drawable/grass2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="37dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:src="@drawable/grass3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="37dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:src="@drawable/grass4"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="37dp"
        app:layout_constraintBottom_toBottomOf="@id/mypage_post_iv"/>

    <ImageView
        android:layout_width="82dp"
        android:layout_height="78dp"
        android:id="@+id/mypage_list_iv"
        android:src="@drawable/list_icon"
        app:layout_constraintEnd_toEndOf="@id/mypage_change_info_iv"
        android:layout_marginTop="580dp"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="23dp"
        android:src="@drawable/list_letter"
        app:layout_constraintEnd_toEndOf="@id/mypage_change_info_iv"
        android:layout_marginTop="605dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="22dp"/>
    


</androidx.constraintlayout.widget.ConstraintLayout>