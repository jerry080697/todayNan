<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">

    <ImageView
        android:layout_width="193dp"
        android:layout_height="66dp"
        android:id="@+id/today_nan_iv"
        android:src="@drawable/splash_icon3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="150dp"
        />
    <ImageView
        android:scaleType="fitXY"
        android:id="@+id/google_btn"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginBottom="15dp"
        android:src="@drawable/google_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="250dp"
        app:layout_constraintTop_toBottomOf="@id/today_nan_iv" />

    <ImageView
        android:scaleType="fitXY"
        android:id="@+id/kakao_btn"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginBottom="15dp"
        android:src="@drawable/kakao_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/google_btn" />

    <ImageView
        android:scaleType="fitXY"
        android:id="@+id/naver_btn"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginBottom="15dp"
        android:src="@drawable/naver_icon"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/kakao_btn" />








</androidx.constraintlayout.widget.ConstraintLayout>